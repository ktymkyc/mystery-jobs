{
  "color": {
    "base": {
      "$value": "#FFFFFF",
      "$type": "color"
    },
    "bg": {
      "gradient1": {
        "$value": "#E5FF63",
        "$type": "color"
      },
      "gradient2": {
        "$value": "#F0FFD0",
        "$type": "color"
      },
      "gradient3": {
        "$value": "#FFFBF0",
        "$type": "color"
      },
      "gradient4": {
        "$value": "#E3DAFF",
        "$type": "color"
      },
      "gradient5": {
        "$value": "#ABC4FF",
        "$type": "color"
      }
    },
    "backgroundGradient": {
      "$value": {
        "stops": [
          {
            "position": 0,
            "color": "#E5FF63"
          },
          {
            "position": 0.15,
            "color": "#F0FFD0"
          },
          {
            "position": 0.6,
            "color": "#FFFBF0"
          },
          {
            "position": 0.8,
            "color": "#E3DAFF"
          },
          {
            "position": 1,
            "color": "#ABC4FF"
          }
        ],
        "gradientType": "linear",
        "rotation": 90
      },
      "$type": "gradient"
    },
    "text": {
      "text": {
        "$value": "#233506",
        "$type": "color"
      },
      "subtext": {
        "$value": "#4E0532",
        "$type": "color"
      }
    },
    "button": {
      "background": {
        "$value": {
          "stops": [
            {
              "position": 0,
              "color": "#FC4CFF"
            },
            {
              "position": 1,
              "color": "#CA00A5"
            }
          ],
          "gradientType": "linear",
          "rotation": 90
        },
        "$type": "gradient"
      },
      "text": {
        "$value": "#ffffff",
        "$type": "color"
      },
      "disabled": {
        "background": {
          "$value": "#E6C1E6",
          "$type": "color"
        },
        "text": {
          "$value": "#F7ECF7",
          "$type": "color"
        }
      }
    }
  },
  "radius": {
    "sm": {
      "$value": "8px",
      "$type": "other"
    },
    "md": {
      "$value": "16px",
      "$type": "other"
    },
    "lg": {
      "$value": "24px",
      "$type": "other"
    }
  },
  "font": {
    "base": {
      "$value": "16px",
      "$type": "other"
    },
    "lg": {
      "$value": "18px",
      "$type": "other"
    },
    "xl": {
      "$value": "24px",
      "$type": "other"
    },
    "2xl": {
      "$value": "32px",
      "$type": "other"
    }
  },
  "lineHeight": {
    "base": {
      "$value": "1.2",
      "$type": "other"
    },
    "tight": {
      "$value": "1",
      "$type": "other"
    },
    "loose": {
      "$value": "1.7",
      "$type": "other"
    }
  }
}